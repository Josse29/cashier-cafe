{
  "expo": {
    "name": "cashier-cafe",
    "slug": "cashier-cafe",
    "scheme": "cashiercafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/icon-app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jossedev.cashiercafe"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed7b0a36-9545-4fe9-9d99-2623f6a2074e"
      }
    }
  }
}
